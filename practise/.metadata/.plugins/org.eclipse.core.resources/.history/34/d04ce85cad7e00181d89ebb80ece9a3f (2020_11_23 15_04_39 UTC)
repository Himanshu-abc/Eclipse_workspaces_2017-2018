package pract;

import java.util.Scanner;

public class Practise16 {

	public static void main(String[] args) {
	
		try {
	  Runtime app=Runtime.getRuntime();
	  
	  //open notepad
	  app.exec("notepad");
	  
	  //open calculator
	  app.exec("Calculator");
	  
	  //open calender
	  app.exec("Calender");
	  
		}catch (Exception e) {
		e.printStackTrace();
		}
	}
	}
